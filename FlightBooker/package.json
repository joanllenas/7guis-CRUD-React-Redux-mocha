{
  "name": "7guis-FlightBooker-React-mocha",
  "description": "7guis FlightBooker exercise made with ReactJS UI library and Mocha tests",
  "version": "1.0.1",
  "main": "./src/index.jsx",
  "engines": {
    "node": "4.2.3"
  },
  "scripts": {
    "build": "node ./node_modules/.bin/webpack",
    "serve": "node ./node_modules/.bin/webpack-dev-server",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.jsx 'test/**/*.spec.@(js|jsx)'",
    "test:watch": "npm run test -- --watch --watch-extensions jsx"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "keywords": [],
  "author": "@joanllenas",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "expect": "^1.13.4",
    "jsdom": "^7.2.2",
    "mocha": "^2.3.4",
    "react": "^0.14.3",
    "react-addons-test-utils": "^0.14.3",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  }
}
