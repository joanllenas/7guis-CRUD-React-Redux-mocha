{
  "name": "7guis-CircleDrawer-React-mocha",
  "description": "7guis Circle drawer exercise made with ReactJS UI library and Mocha tests",
  "version": "1.0.0",
  "main": "./src/index.js",
  "engines": {
    "node": "4.2.3"
  },
  "scripts": {
    "build": "node ./node_modules/.bin/webpack",
    "serve": "node ./node_modules/.bin/webpack-dev-server -d",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js 'test/**/*.spec.@(js|jsx)'",
    "test:watch": "npm run test -- --watch --watch-extensions jsx"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      "react"
    ],
    "plugins": [
      "babel-plugin-rewire"
    ]
  },
  "keywords": [],
  "author": "@joanllenas",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-plugin-rewire": "^1.0.0-beta-5",
    "babel-plugin-syntax-jsx": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.5.0",
    "babel-template": "^6.3.13",
    "babel-types": "^6.4.1",
    "expect": "^1.13.4",
    "jsdom": "^7.2.2",
    "mocha": "^2.3.4",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  }
}
