{
  "name": "7guis-CRUD-React-Redux-mocha",
  "description": "7guis CRUD exercise made with ReactJS, Redux and Mocha tests",
  "version": "1.0.0",
  "main": "./src/index.js",
  "engines": {
    "node": "4.2.3"
  },
  "scripts": {
    "build": "node ./node_modules/.bin/webpack",
    "serve": "node ./node_modules/.bin/webpack-dev-server",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js 'test/**/*.spec.@(js|jsx)'",
    "test:watch": "npm run test -- --watch --watch-extensions jsx"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "keywords": [],
  "author": "@joanllenas",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-plugin-syntax-jsx": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-template": "^6.3.13",
    "babel-types": "^6.4.1",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.2",
    "expect": "^1.13.4",
    "jsdom": "^7.2.2",
    "mocha": "^2.3.4",
    "react": "^0.14.3",
    "react-addons-test-utils": "^0.14.3",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.1.2",
    "redux": "^3.1.2",
    "recompose": "^0.15.0"
  }
}
